- Utilities:
    - Actual:
        icon: actual-budget.png
        href: https://actual.fantaquoggio.bid
        description: Gestisce i sordi
        server: server-docker
        container: actualbudget
        statusStyle: "dot"
        showStats: true

    - Trilium:
        icon: trilium.png
        href: https://trilium.fantaquoggio.bid
