    - beszel:
        icon: beszel.png
        href: http://quoggioserver:8090
        description: monitoring
        server: server-docker
        container: beszel
        statusStyle: "dot"
        showStats: true
        widget:
          type: beszel
          url: http://beszel:8090
          username: "{{ beszel_user }}"
          password: "{{ beszel_password }}"
          systemId: "{{ beszel_sid }}"
          version: 2

    - qBitTorrent:
        icon: qbittorrent.png
        href: http://quoggioserver:8585
        description: Attento ai pornazzi
        server: server-docker
        container: qbittorrent
        statusStyle: "dot"
        showStats: true
        widget:
          type: qbittorrent
          url: http://qbittorrent:8585
          username: "{{ qb_user }}"
          password: "{{ qb_password }}"
          enableLeechProgress: true
          enableLeechSize: true

